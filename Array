1.Second largest number:
class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        if (arr.size() < 2) return -1; // edge case

        int largest = arr[0];
        int seclargest = -1;

        for (int i = 1; i < arr.size(); i++) {
            if (arr[i] > largest) {
                seclargest = largest;
                largest = arr[i];
            } else if (arr[i] < largest && arr[i] > seclargest) {
                seclargest = arr[i];
            }
        }

        return seclargest;
    }
};

**Reverse a number:***

#include<iostream>
using namespace std;

void reverse(int n, int arr[]) {
    int start = 0;
    int end = n - 1;
    while(start <= end) {
        swap(arr[start], arr[end]);
        start++;
        end--;
    }
}

void printArray(int n, int arr[]) {
    for(int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

int main() {
    int n;
    cout << "Enter the number: ";
    cin >> n;

    int arr[n];
    cout << "Enter the elements: ";
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    cout << "Original array: ";
    printArray(n, arr);
    reverse(n, arr);
    cout << "Reversed array: ";
    printArray(n, arr);
    return 0;
}

2. sec largest 
#include <iostream>
#include <vector>
using namespace std;

int seclargestnum(vector<int> &arr) {
    if (arr.size() < 2) return -1;

    int largest = arr[0];
    int seclargest = -1;

    for (int i = 1; i < arr.size(); i++) {
        if (arr[i] > largest) {
            seclargest = largest;
            largest = arr[i];
        } else if (arr[i] < largest && arr[i] > seclargest) {
            seclargest = arr[i];
        }
    }
    return seclargest;
}

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    vector<int> arr(n);
    cout << "Enter elements: ";
    for (int i = 0; i < n; i++) cin >> arr[i];

    int result = seclargestnum(arr);

    cout << "The array is: ";
    for (int i = 0; i < n; i++) cout << arr[i] << " ";
    cout << endl;

    if (result != -1)
        cout << "Second largest number is: " << result << endl;
    else
        cout << "No second largest number found." << endl;

    return 0;
}
3. second largest and smallest 
#include <iostream>
#include <vector>
#include <climits>
using namespace std;

class Solution {
public:
    void getSecondLargestAndSmallest(vector<int>& arr, int n) {
        if (n < 2) {
            cout << "Not enough elements." << endl;
            return;
        }

        int largest = INT_MIN, secondLargest = INT_MIN;
        int smallest = INT_MAX, secondSmallest = INT_MAX;

        // First pass: Find largest and smallest
        for (int i = 0; i < n; i++) {
            if (arr[i] > largest)
                largest = arr[i];
            if (arr[i] < smallest)
                smallest = arr[i];
        }

        // Second pass: Find second largest and second smallest
        for (int i = 0; i < n; i++) {
            if (arr[i] > secondLargest && arr[i] < largest)
                secondLargest = arr[i];
            if (arr[i] < secondSmallest && arr[i] > smallest)
                secondSmallest = arr[i];
        }

        if (secondLargest == INT_MIN)
            cout << "No second largest (all elements same or only one distinct)." << endl;
        else
            cout << "Second Largest: " << secondLargest << endl;

        if (secondSmallest == INT_MAX)
            cout << "No second smallest (all elements same or only one distinct)." << endl;
        else
            cout << "Second Smallest: " << secondSmallest << endl;
    }
};

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    vector<int> arr(n);
    cout << "Enter " << n << " elements: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    Solution sol;
    sol.getSecondLargestAndSmallest(arr, n);

    return 0;
}

4. to check the array is sorted or not.
#include <iostream>
#include <vector>
using namespace std;

bool issorted(int n, vector<int> arr) {
    for (int i = 1; i < n; i++) {
        if (arr[i] < arr[i - 1]) {
            return false;
        }
    }
    return true;
}

5. remove duplicate elements
#include <iostream>
#include <vector>
using namespace std;

// Remove duplicates from a sorted array and return new length
int removedup(int n, vector<int>& arr) {
    if (n == 0) return 0;

    int i = 0;
    for (int j = 1; j < n; j++) {
        if (arr[i] != arr[j]) {
            i++;
            arr[i] = arr[j];
        }
    }
    return i + 1;
}
6. swap alternative array 
#include<iostream>
using namespace std;

void swapalternate(int n, int arr[]) {
    for(int i = 0; i < n - 1; i += 2) {
        swap(arr[i], arr[i + 1]);
    }
}

void printarray(int n, int arr[]) {
    for(int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

int main() {
    int n;
    cout << "Enter the number of elements: ";
    cin >> n;

    int arr[n];
    cout << "Enter the elements: ";
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "Original array: ";
    printarray(n, arr);

    swapalternate(n, arr);

    cout << "Array after swapping alternate elements: ";
    printarray(n, arr);

    return 0;
}

7.find unique value form a array:
#include<iostream>
using namespace std;

int findUnique(int n, int arr[]) {
    int ans = 0;
    for(int i = 0; i < n; i++) {
        ans = ans ^ arr[i];  // XOR operation to cancel out duplicates
    }
    return ans;
}

void printarray(int n, int arr[]) {
    for(int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

int main() {
    int n;
    cout << "Enter the number of elements: ";
    cin >> n;

    int arr[n];
    cout << "Enter the elements: ";
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "Original array: ";
    printarray(n, arr);

    int uniqueElement = findUnique(n, arr);
    cout << "The unique element is: " << uniqueElement << endl;

    return 0;
}


